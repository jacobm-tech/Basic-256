#string
if "ZZ" = "ZZ" then print "pass"
if "ZZ" = "XX" then print "fail"
if "XX" < "ZZ" then print "pass"
if "ZZ" < "XX" then print "fail"
if "ZZ" > "XX" then print "pass"
if "XX" > "ZZ" then print "fail"
